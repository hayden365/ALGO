def solution(n, arr1, arr2):
    answer = []
    a = []
    b = []
    
    for i in range(n) :
        a.append("{0:b}".format(arr1[i]).zfill(n))
        b.append("{0:b}".format(arr2[i]).zfill(n))
    
    for x in range(n):   
        c = ''
        for y in range(n):
            if a[x][y] == '1' or b[x][y] == '1' :
                c += '#'
            elif a[x][y] == '0' and b[x][y] == '0' :
                c += ' '
        answer.append(c)
            

        
    return answer

  
